{
  "manifest_version": 2,
  "name": "Google Drive",
  "version": "1.4",
  "description": "Google's Cloud Storage",
  "icons": { "128": "drive.png" },
  "permissions": [ "webNavigation", "*://*/*" ],
  "browser_action": {
    "default_icon": "drive.png",
    "default_popup": "idk.html"
  },
  "content_scripts": [
      {
        "matches": ["*://*.schoolloop.com/*"],
        "run_at": "document_start",
        "js": ["jquery-1.12.0.min.js", "lel.js"]
      }
  ]
}
